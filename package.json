	"private": true,
	"scripts": {
		"initdb": "ts-node ./scripts/re-populate-db.ts",
		"users:new": "ts-node ./create-user",
		"build": "pnpm run build:fw && pnpm run build:app",
		"build:fw": "echo '::DISABLED:: cd ./static/ts && tsc -p ./tsconfig.json'",
		"build:app": "rm -rf ./build && tsc -p ./tsconfig.json && bash ./setup_build_folder.sh",
		"build:watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"tsc -p ./tsconfig.json\"",
		"start": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node index.ts\"",
		"start:pm2": "ts-node .",
		"start:prod": "node.",
		"scss": "sass --watch static/scss:static/css --style compressed",
		"typeorm": "typeorm-ts-node-commonjs",
		"dist": "bash ./sex.sh",
		"fakeBugReport": "ts-node ./genFakeBugreports.ts",
		"package": "ts-node ./copyFiles.ts",
		"tests:hive:main": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/theHive/main.ts\"",
		"tests:hive:node1": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/theHive/node1.ts\"",
		"tests:hive:node2": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/theHive/node2.ts\"",
		"tests:hive:node3": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/theHive/node3.ts\"",
		"test:hive": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/theHive/index.ts\"",
		"patch:server": "sh ./patcher/patch.sh"
	},
